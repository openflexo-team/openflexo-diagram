				<item name="Diagram" href="flexodiagram/index.html" />
			</item>
			<item name="UI layer" href="openflexo-technology-adapters-ui/index.html">
				<item name="Diagram-UI" href="flexodiagram-ui/index.html" />
			</item>
			<item name="FAQ" href="faq.html" />
		</menu>

		<menu ref="modules" />
		<menu ref="reports" />
